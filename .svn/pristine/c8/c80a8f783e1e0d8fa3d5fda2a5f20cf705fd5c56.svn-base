package ir.aria.pelaksefid.domain.model;

import ir.aria.pelaksefid.client.consume.sigma.ManaPrice;
import lombok.Getter;
import lombok.Setter;

/**
 *
 * @author Mana2
 */
@Getter
@Setter
public class ManaPriceDto {

    private String id;
    private String carModel;
    private String carModelPersian;
    private String updatePriceDate;
    private String mostPrice;
    private String leastPrice;
    private String imagePath;
    private String brandId;
    private String isActive;

    public void fromModel(ManaPrice m) {
        id = m.getId();
        carModel = m.getCarModel();
        carModelPersian = m.getCarModelPersian();
        updatePriceDate = m.getUpdatePriceDate();
        mostPrice = m.getMostPrice();
        leastPrice = m.getLeastPrice();
        imagePath = m.getImagePath();
        brandId = m.getBrandId();
        isActive = m.getIsActive();
    }
}
